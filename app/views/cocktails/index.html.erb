<h1>Show all the cocktails</h1>
<ul>
  <% @cocktails.each do |cocktail| %>
  <li>
    <%= cl_image_tag(cocktail.photo,
    width: 400, height: 300, crop: :fill) %>
    <%= link_to cocktail.name, cocktail_path(cocktail)  %>
  </li>
  <% end  %>
  <%= link_to 'create cocktail', new_cocktail_path  %>
</ul>